<%- include("_header"); %>
<div class="container-md">
  <div class="row mt-4">
    <%- include("_tables"); %>
    <div class="col-10">
      <%- include("_tab", {tab: "structure"}); %>

      <div class="clearfix mb-4">
        <h4 class="float-start">Table: <%=tableName%></h4>
        <a class="float-end btn btn-primary"
           href="/add-column?tableName=<%=tableName%>"
           role="button">
          Add column</a>
      </div>

      <%- include("_columns") %>

      <h4 class="mb-4">SQL</h4>
      <pre><code class="fs-6"><%=sqlDef%></code></pre>
      
    </div>
  </div>
</div>
<%- include("_footer"); %>
